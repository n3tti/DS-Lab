    <div class="textimage">






    
        
        
            
            
                <article id="eda60f266" class="clearfix article-print">
            
        

        
        
            <figure class="margin-right-25 float-left span2
            
            ">
                
                    
                        
                        
                            <img src="/content/dam/eda/img/6-Dienstleistungen-Publikationen/2023-swissintouch-ratgeber-EDA.jpg/jcr:content/renditions/cq5dam.thumbnail.180.270.png"
